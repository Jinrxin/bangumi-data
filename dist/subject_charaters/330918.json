es":{"small":"https://lain.bgm.tv/r/100/pic/crt/l/99/5f/101783_crt_502dD.jpg?r=1634054450","grid":"https://lain.bgm.tv/r/200/pic/crt/l/99/5f/101783_crt_502dD.jpg?r=1634054450","large":"https://lain.bgm.tv/pic/crt/l/99/5f/101783_crt_502dD.jpg?r=1634054450","medium":"https://lain.bgm.tv/r/400/pic/crt/l/99/5f/101783_crt_502dD.jpg?r=1634054450"},"name":"フランツ・フェルマン","relation":"配角","actors":[{"images":{"small":"https://lain.bgm.tv/r/100/pic/crt/l/d3/3e/13744_prsn_1XrSS.jpg","grid":"https://lain.bgm.tv/r/200/pic/crt/l/d3/3e/13744_prsn_1XrSS.jpg","large":"https://lain.bgm.tv/pic/crt/l/d3/3e/13744_prsn_1XrSS.jpg","medium":"https://lain.bgm.tv/r/400/pic/crt/l/d3/3e/13744_prsn_1XrSS.jpg"},"name":"山谷祥生","short_summary":"日本男性声优。隶属于WITH LINE，前SPACE CRAFT成员。出身于宫城县。\r\n以声优为梦想，便从大学中途退学。于东京动画学院声优演员科毕业。2013年在电视动画《闪乱神乐》出道。2014年在《一周的朋友》中首次饰演主角。\r\n憧憬的声优是福山润。","career":["seiyu"],"id":13744,"type":1,"locked":false}],"type":1,"id":101783},{"images":{"small":"https://lain.bgm.tv/r/100/pic/crt/l/d5/33/122935_crt_AdT9Q.jpg","grid":"https://lain.bgm.tv/r/200/pic/crt/l/d5/33/122935_crt_AdT9Q.jpg","large":"https://lain.bgm.tv/pic/crt/l/d5/33/122935_crt_AdT9Q.jpg","medium":"https://lain.bgm.tv/r/400/pic/crt/l/d5/33/122935_crt_AdT9Q.jpg"},"name":"サングラード","relation":"客串","actors":[],"type":4,"id":122935},{"images":{"small":"https://lain.bgm.tv/r/100/pic/crt/l/bf/6d/122936_crt_31nGM.jpg","grid":"https://lain.bgm.tv/r/200/pic/crt/l/bf/6d/122936_crt_31nGM.jpg","large":"https://lain.bgm.tv/pic/crt/l/bf/6d/122936_crt_31nGM.jpg","medium":"https://lain.bgm.tv/r/400/pic/crt/l/bf/6d/122936_crt_31nGM.jpg"},"name":"ツィルニトラ共和国","relation":"客串","actors":[],"type":4,"id":122936},{"images":{"small":"https://lain.bgm.tv/r/100/pic/crt/l/39/a9/122937_crt_M7FYQ.jpg","grid":"https://lain.bgm.tv/r/200/pic/crt/l/39/a9/122937_crt_M7FYQ.jpg","large":"https://lain.bgm.tv/pic/crt/l/39/a9/122937_crt_M7FYQ.jpg","medium":"https://lain.bgm.tv/r/400/pic/crt/l/39/a9/122937_crt_M7FYQ.jpg"},"name":"アーナック連合王国","relation":"客串","actor